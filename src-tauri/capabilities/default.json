{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api.notion.com/v1/**"
        }
      ]
    },
    "fs:default",
    "fs:allow-appconfig-write",
    "fs:allow-appconfig-read"
  ]
}
